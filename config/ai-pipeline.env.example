# AI Servicing Pipeline Environment Configuration
# Copy to .env and configure your values

# Message Queue Configuration
AMQP_URL=amqps://YOUR_RABBIT_URL

# AWS Configuration
AWS_REGION=us-east-1
S3_BUCKET=loanserve-docs
S3_PREFIX=tenants

# AI Processing Thresholds
SPLIT_COSINE_THETA=0.35
CLASSIFIER_DELTA=0.18
OCR_MIN_CONF_PAGE=0.80
OCR_TIMEOUT_MS=180000

# Pipeline Versioning
EXTRACTOR_VERSION=v2025.09.01

# Confidence Thresholds
CONF_ACCEPT=0.80
CONF_HITL=0.60

# Program Configuration
PROGRAM=FNMA
INVESTOR_PROFILE=DEFAULT

# Environment
NODE_ENV=production

# LLM Provider Configuration
LLM_PROVIDER=mock
LLM_MODEL=gpt-4o-mini
OPENAI_API_KEY=

# AI Runtime Configuration
AI_TEMPERATURE=0.0
AI_MAX_TOKENS=2000
AI_REQUEST_TIMEOUT_MS=60000
AI_SLICE_TARGET_CHARS=8000
AI_MIN_CONFIDENCE=0.60
AI_ACCEPT_CONFIDENCE=0.80

# X.AI API for Document Processing
XAI_API_KEY=your_xai_api_key_here

# Phase 10 Security (already configured)
PHASE10_MASTER_KEY=your_master_encryption_key

# DocuSign Integration (for document custody)
DOCUSIGN_WEBHOOK_SECRET=your_docusign_webhook_secret